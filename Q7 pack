package evenpackage;
import java.util.*;

public class even {
    static int n;
    public void getno(int no)
    {
        n=no;
    }

    public static void check()
    {
        if(n%2==0)
        {
            System.out.println("No is even");
        }
        else
        {
            System.out.println("No is odd");
        }
    }

    public static void main(String args[])
    {

    }
}
